    function Binddate(Id, title) {
        $("#customs-groups tr td a").removeClass("active-bold");
        $(`#${Id}`).addClass("active-bold");

        $("#txttitlevalue").text(title);
        $("#CommunityBookingId").val(Id);
        var objData = {
            MasterBookingId: Id
        }
        var Aurl = "/Planner/GetBookingDays";
        $.ajax({
            type: "Post",
            url: Aurl,
            url: Aurl,
            data: objData,
            dataType: "json",
            success: function (Result) {
                // console.log(Result);
                var status = Result.success;
                var lstUsers = [];
                if (status == true) {
                    for (i = 0; i < Result.data.length; i++) {
                        //  alert(lstUsers);
                        lstUsers.push("<option value=" + Result.data[i].Id + ">" + Result.data[i].BookingDays + " </option>")
                    }
                    document.getElementById("ddlE").innerHTML = "<option value='0'>Select a date</option>" + lstUsers;
                }
            },
            error: function (request, error) {
                $("#Items-Added").modal("show");
                $("#itemaddheader").text('Something went wrong!');
                $("#message").text(error);
            }

        });
    }
