                    </ul>
                </div>
            </div>
            <div class="col-lg-10">

                <div class="tab-content">
                    <!-- @* =====Upcoming Event=====*@ -->
                    <div id="UpcomingEvent" class="tab-pane fade active show ">
                        <div class="cross-icon text-right d-block cross-relative planner-cross" id="Upcomingcross">

                            <button type="button" class="cross">
                                <span aria-hidden="true">×</span>
                            </button>
                        </div>

                        <!-- For UpcomingEvent purpose-->
                        <!-- Users box-->
                        <div class=" border-bx-area border-0">
                            <div class="table-responsive ">
                                <table id="UpcomingTable" class="table">
                                    <thead>
                                        <tr>
                                            <th><span>Event title</span> <span class="ml-2 position-absolute"><img src="~/images/Filter1.png"></span></th>
                                            <th><span>Event date</span> <span class="ml-2 position-absolute"><img src="~/images/Filter1.png"></span></th>
                                            <th>Ticket Price</th>
                                            <th><span>Amount collected</span> <span class="ml-2 position-absolute"><img src="~/images/Filter1.png"></span></th>
                                            <th>Registered</th>
                                            <th>Checkins</th>
                                            <th>Attached PDF</th>
                                            <th>Delete event</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var item in Model.UpcomingEvents)
                                        {
                                            <tr>
                                                <td><span class="text-over">@item.Title</span><span class="ml-3 position-absolute" onclick="BindUpcoming(@item.Id)"><img src="~/images/Show.png"></span></td>
                                                <td><span>@item.EventStartDate.ToString("MM.dd.yyyy") -- @item.EventEndDate.ToString("MM.dd.yyyy")</span></td>
                                                <td>
                                                    @Model.currencyModel.CurrencyCode  @item.TicketPrice

                                                </td>
                                                <td> @Model.currencyModel.CurrencyCode @item.AmountCollected</td>
                                                <td>@item.PaidCount/@item.TotalMembersAtCreation</td>
                                                <td><span class="" onclick="ShowReviewAttendance(@item.Id, '@item.Title')"><img src="~/images/Location-Tick.png"></span></td>                          
                                                <td>
                                                    @if (!string.IsNullOrEmpty(item.EventPdf))
                                                    {
                                                        <a href="@item.EventPdf" target='_blank'><img src='/images/attached-pdf.png' alt='Download' /></a>
                                                    }
                                                    else
                                                    {
                                                        <span></span>
                                                    }
                                                </td>

                                                <td><span class=""><img onclick="deleteUpcomingActive(@item.Id)" src="~/images/Delete2.png"></span></td>
                                            </tr>
                                        }

                                    </tbody>
                                </table>
                            </div>
                        </div>

                    </div>
