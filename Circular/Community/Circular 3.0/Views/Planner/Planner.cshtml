        //}
        if (Bookingtitle == "") {
            $('#txtBooking').focus();
            document.getElementById("txtBooking").style.border = "1px solid red";
            return false;
        }

        if (Startdate == "") {
            $('#txtstartdate').focus();
            document.getElementById("txtstartdate").style.border = "1px solid red";
            return false;
        }

        if (Enddate == "" && Days != "") {
            $('#txtenddate').focus();
            document.getElementById("txtenddate").style.border = "1px solid red";
            return false;
        }
        if (Enddate != "" && Days == "") {
            $('#txtstartdate').focus();
            document.getElementById("txtenddate").style.border = "1px solid red";
            return false;
        }

        if (StartTime == "") {
            $('#txtStarttime').focus();
            document.getElementById("txtStarttime").style.border = "1px solid red";
            return false;
        }
        if (EndTime == "") {
            $('#txtEndtime').focus();
            document.getElementById("txtEndtime").style.border = "1px solid red";
            return false;
        }
        if (SlotAvailability == "") {
            $('#txtSlotAvail').focus();
            document.getElementById("txtSlotAvail").style.border = "1px solid red";
            return false;
        }
        formData = new FormData();
        formData.append("Title", Bookingtitle);
        formData.append("Days", Days);
        formData.append("StartDate", CombinedstartDate);
        formData.append("EndDate", CombinedEndtDate);
