            var dayId = "," + $(this).attr('id');
            var pos = recurringDays.indexOf(dayId);
            var secondPos = pos + 2;
            var secondString = recurringDays.substring(secondPos);
            var firstString = recurringDays.substring(0, pos);
            recurringDays = firstString + secondString;
        }
        else {
            $(this).addClass('active');
            $(this).css('background-color', '#253347');
            recurringDays += "," + $(this).attr('id');
        }
    });
