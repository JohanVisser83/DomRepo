        var CombinedstartDate = Startdate + ' ' + StartTime;
        var CombinedEndtDate = Enddate + ' ' + EndTime;

        //     alert(ActivateNotification);
        //if (Days == undefined || Days == '' || Days == null) {
        //    alert('ok');
        //    $('.week-bx').focus();
        //    document.getElementsByClassName("week-bx").style.border = "1px solid red";
