        formData.append("EndTime", CombinedEndtDate);
        formData.append("Email", HostEmailname);
        formData.append("Hostname", Hostname);
        formData.append("IsNotificationActive", ActivateNotification);

        var Aurl = "/Planner/AddBooking";
        $.ajax({
            type: "Post",
            url: Aurl,
            cache: false,
            contentType: false,
            processData: false,
