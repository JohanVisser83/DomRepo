@model CircularWeb.Models.SportsModel;
@{
    Layout = "_Layout";
}
<div class="col-lg-11 col-md-11  col-sm-10 col-10">
    <div class="body-content">
        <div class="row">
            <div class="col-lg-2">
                <div class="sidebar mt-38">
                    <ul class="nav nav-tabs tab-data message-tab">
                        <li>
                            <p class="tab-text">Sports</p>
                        </li>
                        <li><a data-toggle="tab" href="#SportsManagement" class="active">Sports management</a></li>
                        <li><a data-toggle="tab" href="#SportsTypeActiveBtn">Sports Type</a></li>

                    </ul>
                </div>
            </div>
            <div class="col-lg-10">
                <div class="tab-content ">
                    <!-- @* =====SportsManagement Groups =====*@ -->
                    <div id="SportsManagement" class="tab-pane fade new-event active show">
                        <div class="safty-top-area">
                            <div class="row align-items-center">
                                <div class="ml-auto cross-icon text-right  cross-relative planner-cross" id="Sportscross">
                                    <button type="button" class="cross ml-3">
                                        <span aria-hidden="true">×</span>
                                    </button>
                                </div>
                            </div>
                        </div>
                        <div class="row custom-group-area ">
                            <!--  @* ====col-4====*@ -->
                            <!-- @*=====end====*@ -->
                            <!-- @* ====col-8===*@ -->
                            <div class="col-lg-12 ">
                                <div class=" border-bx-area">
                                    <ul class="nav nav-tabs sports-tabs">
                                        <li><a data-toggle="tab" href="#Upcoming" id="activeupcoming">Upcoming</a></li>
                                        <li><a data-toggle="tab" href="#Results">Results</a></li>

                                    </ul>
                                    <div class="tab-content pb-0  ">

                                        <!-- ====end==== -->
                                        <!-- @* =====Team=====*@ -->
                                        <div id="Upcoming" class="tab-pane fade active show ">
                                            <div class="table-responsive border-radius-0">
                                                <table class="table " id="upcoming-table">
                                                    <thead>
                                                        <tr>
                                                            <th style="display:none">Day</th>
                                                            <th><span>Date</span><span class="ml-2"><img src="~/images/Filter1.png"></span></th>
                                                            <th>Event</th>
                                                            <th>Delete</th>
                                                        </tr>
                                                    </thead>
                                                </table>
                                            </div>
                                            <button type="button" class="btn btn-block btn-link btn-lumo " data-target="#AddNewSport" data-toggle="modal"><span class="mr-2"><img src="~/images/Add-Circle.png"></span>Add a new event</button>
                                        </div>
                                        <!--  @*==end===*@ -->
                                        <!-- @* =====Fixtures=====*@ -->
                                        <div id="Results" class="tab-pane fade ">
                                            <div class="table-responsive border-radius-0">
                                                <table class="table " id="results-table">
                                                    <thead>
                                                        <tr>
                                                            <th style="display:none">Day</th>
                                                            <th><span>Date</span><span class="ml-2"><img src="~/images/Filter1.png"></span></th>
                                                            <th>Event</th>
                                                            <th>Delete</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                    </tbody>
                                                </table>
                                            </div>

                                        </div>
                                        <!--  @*==end===*@ -->

                                    </div>
                                </div>
                            </div>
                            <!-- @* ====end=====*@ -->
                        </div>
                    </div>
                    <!-- @*==end===*@ -->

                    <div id="SportsTypeActiveBtn" class="tab-pane fade new-event  ">
                        <div class="safty-top-area">
                            <div class="row align-items-center">
                                <div class="ml-auto cross-icon text-right  cross-relative planner-cross">
                                    <button type="button" class="cross ml-3" id="btnCrossSportsId">

                                        <span aria-hidden="true">×</span>

                                    </button>
                                </div>
                            </div>
                        </div>
                        <div class="row custom-group-area ">
                            <!--  @* ====col-4====*@ -->
                            <div class="col-lg-12">
                                <div class=" border-bx-area  ">
                                    <div class="table-responsive border-radius-0">
                                        <table id="SportsType_groups" class="table ">
                                            <thead>
                                                <tr>
                                                    <th>Activites</th>
                                                    <th>Delete</th>
                                                    <th>Edit</th>
                                                </tr>
                                            </thead>

                                            <tbody>
                                                @foreach (var item in Model.lstactivity)
                                                {

                                                    <tr>
                                                        <td editValue="@item.Id" class="lblsports">@item.Activities</td>
                                                        <td onclick="DeleteSportTypeId(@item.Id)"><img src="~/images/Delete2.png"></td>
                                                        <td onclick="EditSportTypeId(@item.Id)"><img src="/images/edit.png" /></td>

                                                    </tr>



                                                }

                                            </tbody>
                                        </table>
                                    </div>
                                    <button type="button" class="btn btn-block btn-link btn-lumo " data-target="#Adding-Sport_Type" data-toggle="modal"><span class="mr-2"><img src="~/images/Add-Circle.png"></span>Add New Sports Type</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- @*==end===*@ -->
                </div>





            </div>

            <!-- @* =====SportsManagement Groups =====*@ -->


        </div>
    </div>
</div>

<div class="overlay"></div>



<!-- @*======Add New Sport====*@ -->
<div class="modal fade modal-add-bx" id="AddNewSport" tabindex="-1" aria-hidden="true">
    <div class="modal-dialog modal-dialog-scrollable w-500 modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title ">Add a new event</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    &times;
                </button>
            </div>
            <div class="modal-body">
                <div class="new-message">
                    <div class="col-md-12">



                        <div class="form-group ">
                            <label>What is the name of this event?</label>
                            <input type="text" class="form-control" id="txtnamesports">
                        </div>

                        <div class="form-group">
                            <label>Select a date for this event</label>
                            <div class=" date-icon">
                                <input type="text" id="datetime2" class="form-control  btn-date">
                            </div>
                        </div>


                        <div class="form-group">
                            <label>Upload a PDF for this event</label>
                            <div class=" add-icon-bx" id="logo-uploadAccount">
                                <label for="pdfcommunitys" class="custom-file-upload nameimage" id="uploadSec3">
                                    <span></span>
                                </label>

                                <div class="add-icon upload-icon">
                                    <input id="pdfcommunitys" type="file" accept=".pdf,.doc" onchange="ProductreadURLS12(this)">
                                    <img src="~/images/Add-Circle.png">
                                </div>
                            </div>
                        </div>

                        <div class="form-group ">
                            <label>Cover Image</label>
                            <div class=" add-icon-bx">

                                <input type="text" id="txtCoverImage" class="form-control" disabled>
                                <div class="add-icon">
                                    <input type="file" id="CoverImage" accept="txtCoverImage/*" onchange="CoverreadURL(this);" class="form-control">
                                    <img src="~/images/add-image-icon.png">
                                </div>
                            </div>
                        </div>

                    </div>
                    <div class="col-md-12 text-right">
                        <button type="button" class="btn btn-lumo-green" id="btnsubmitsports">Next</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- @*===end====*@ -->
<!-- @*=====modal popup ==*@ -->
<!-- Add a new Event -->
<div class="modal fade modal-add-bx small-datatable" id="AddNewEvent" tabindex="-1" aria-labelledby="AttendeesLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title " id="eventtitlename">Manage fixtures</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    &times;
                </button>
            </div>
            <div class="safty-top-area p-4 ">
                <div class="row align-items-center">
                    <div class="col-md-3">

                        <div class="form-group ">
                            <label>Fixture title</label>
                            <input type="text" class="form-control " id="txtTitleFixture">
                        </div>

                    </div>
                    <div class="col-md-3">
                        <div class="form-group ">
                            <label>Start time</label>
                            <input type="time" class="form-control " id="txtStartDate">
                        </div>
                    </div>

                    <div class="col-md-3">
                        <div class="form-group ">
                            <label>Location or Venue</label>
                            <input type="text" class="form-control " id="txtlocation">
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-3">
                        <div class="form-group ">
                            <label>Home Team</label>
                            <input type="text" class="form-control " id="txthometeam">
                        </div>
                    </div>

                    <div class="col-md-3">
                        <div class="form-group ">
                            <label>Away Team</label>
                            <input type="text" class="form-control " id="txtawayteam">
                        </div>
                    </div>

                    <div class="col-md-3">
                        <div class="form-group ">
                            <label>Sport Type</label>
                            <select class="form-control" id="txtsportsid" asp-items="@(new SelectList(Model.lstactivity, "Id", "Activities"))">
                                <option selected>Select Sport Type</option>


                            </select>
                        </div>
                    </div>

                    <div class="col-md-3">
                        <div class="form-group ">
                            <button type="button" class="btn btn-lumo-green btn-block mt-4 py-3" id="Btnaddfixture">Add fixture</button>
                        </div>
                    </div>

                </div>
            </div>
            <div class="table-responsive px-4 pb-4 table-gray-head">
                <table id="Managefixtures" class="table ">
                    <thead>
                        <tr>
                            <th>Fixture title</th>
                            <th>Start time</th>
                            <th>Location</th>
                            <th>Home team</th>
                            <th>Away team</th>
                            <th>Sport Type</th>

                        </tr>
                    </thead>
                    <tbody id="UpcomingView">
                    </tbody>

                </table>
            </div>
        </div>

    </div>

</div>


<!-- @*======end====*@ -->
