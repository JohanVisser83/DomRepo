@model CircularWeb.Models.BaseModel
<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Basic Page Needs
       ================================================== -->
    <meta charset="utf-8">
    <link rel="icon" href="~/images/circular_update1.png">
    <title>Community Central - Circular</title>
  

    <!-- Mobile Specific Metas
       ================================================== -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <!-- CSS
       ================================================== -->
    <!-- Bootstrap -->
    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/Loader.css" />
    <!-- FontAwesome -->
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/jquery-ui.css">
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/emojionearea/3.4.2/emojionearea.min.css"/>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@100..900&display=swap" rel="stylesheet">

    <!-- Javascript Files
         ================================================== -->
    <script type="text/javascript" src="~/js/jquery.min.js"></script>
    <script  src="~/js/cropbox.js"></script>
    <script type="text/javascript" src="~/js/bootstrap.min.js"></script>
   
    <script src="~/js/jquery-ui.js" type="text/javascript"></script>

    <script src="~/js/jquery.dataTables.min.js" type="text/javascript"></script>

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css.map">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/FileSaver.js/2.0.0/FileSaver.min.js"></script>
    @*<script src="https://cdn.rawgit.com/rainabba/jquery-table2excel/1.1.0/dist/jquery.table2excel.min.js"></script>*@
    <script src="https://cdn.jsdelivr.net/npm/jquery-table2excel@1.1.1/dist/jquery.table2excel.min.js"></script>
    <script src="~/js/html5-qrcode.min.js"></script>

    <!---- Button loadder -->

    <link href="~/css/buttonLoader.css" rel="stylesheet">
    

    <link href="~/css/buttonLoader.css" rel="stylesheet" />
    <script type="text/javascript" src="~/js/jquery.buttonLoader.js"></script>
    <script type="text/javascript" src="~/js/jquery.buttonLoader.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/emojionearea/3.4.2/emojionearea.min.js"></script>


    <!-- rcropper -->
    
    <script type="text/javascript" src="~/js/rcrop.js"></script>
    <script type="text/javascript" src="~/js/rcrop.min.js"></script>
    <link href="~/css/rcrop.min.css" rel="stylesheet">
    <link href="~/css/rcrop.css" rel="stylesheet">

    <!-- End -->


</head>
<body>
    <div class="wrapper">
        <div class="container-fluid po">
            <div class="wraper-inner">
                <div class="row no-gutters">
                    <div class="col-lg-1 col-md-1  col-sm-2 col-2">
                        <div class="dashboard-sidebar">
                           <div class="sidbar-hover">
                            <div class="sidebar-header">
                                <img src=@Model.CommunityLogo />
                            </div>
                            <ul>
                                <li>
                                    <a id="first" href="~/Dashboard/Dashboard" class="Dashboard">
                                        <img src="~/images/Category.png" /><img src="~/images/Category-blue.png" /><span class="tooltiptext">Dashboard</span>
                                    </a>
                                </li>
                                    @if (Model.IsFeatureAvailable("M-003"))
                                    {
                                        <li>
                                            <a id="second" href="@Url.Action("Message","Message")" class="Messages">
                                                <img src="~/images/message.png" />
                                                <img src="~/images/Message-blue.png" /><span class="tooltiptext">Communication</span>
                                            </a>
                                        </li>
                                    }
                                    @if (Model.IsFeatureAvailable("P-001"))
                                    {
                                        <li>
                                            <a id="third" href="~/Planner/" class="Calender" data-toggle="tooltip" data-placement="right" >
                                                <img src="~/images/Calender.png" /><img src="~/images/Calender 2.png" /><span class="tooltiptext">Planner</span>
                                            </a>
                                        </li>
                                    }
                                    @if (Model.IsFeatureAvailable("AC-006"))
                                    {
                                        <li>
                                            <a id="four" href="~/Account/Account" class=" Collection" data-toggle="tooltip" data-placement="right" >
                                                <img src="~/images/accounts-white.png" /><img src="~/images/accounts-blue.png" /><span class="tooltiptext">Accounts</span>
                                            </a>
                                        </li>
                                    }
                                    @if (Model.IsFeatureAvailable("S-002"))
                                    {
                                        <li>
                                            <a id="fifth" href="~/Safety/Safety" class="Safety" data-toggle="tooltip" data-placement="right" >
                                                <img src="~/images/Password 1.png" />
                                                <img src="~/images/Password-blue.png" /><span class="tooltiptext">Safety & Tracking</span>
                                            </a>
                                        </li>
                                    }
                                    @if (Model.IsFeatureAvailable("C-004"))
                                    {
                                        <li>
                                            <a id="sixth" href="~/Community/Community" class="Community" data-toggle="tooltip" data-placement="right" >
                                                <img src="~/images/Profile Square.png" />
                                                <img src="~/images/Profile Square-blue.png" /><span class="tooltiptext">Community</span>
                                            </a>
                                        </li>
                                    }
                                    @if (Model.IsFeatureAvailable("SF-005"))
                                    {
                                        <li>
                                            <a id="seventh" href="~/Storefront/Storefront" class="Storefront" data-toggle="tooltip" data-placement="right" >
                                                <img src="~/images/Bag 6.png" />
                                                <img src="~/images/Bag 6-blue.png" /><span class="tooltiptext">Storefront</span>
                                            </a>
                                        </li>
                                    }
                                    @if (Model.IsOwner)
                                    {
                                        <li>
                                            <a id="Nine" href="~/Finance/Finance" class="Finances" data-toggle="tooltip" data-placement="right" >
                                                <img src="~/images/Wallet-white.png" />
                                                <img src="~/images/Wallet-blue.png" /><span class="tooltiptext">Finance Hub</span>
                                            </a>
                                        </li>
                                    }
                                    @if (Model.IsFeatureAvailable("SP-007"))
                                    {
                                        <li>
                                            <a id="Nine" href="~/sports/sports" class="Sports" data-toggle="tooltip" data-placement="right" >
                                                <img src="~/images/Award.png" />
                                                <img src="~/images/Award-blue.png" /><span class="tooltiptext">Sports</span>
                                            </a>
                                        </li>
                                    }
                                    @if (Model.IsOwner)
                                    {
                                        <li>
                                            <a id="ten" href="~/Setting/Setting" class="Setting" data-toggle="tooltip" data-placement="right" >
                                                <img src="~/images/Setting 2.png" />
                                                <img src="~/images/Setting-blue.png" /><span class="tooltiptext">Settings</span>
                                            </a>
                                        </li>
                                    }
                                    <li>
                                    <a id="Eleven" href="@Url.Action("Logout", "Login")"  class="col-pink mt-116">
                                        <img src="~/images/logout-purple.png">
                                        <span class="tooltiptext">Logout</span></a></li>
                            </ul>
                            </div>
                        </div>
                    </div>
                    @RenderBody()
                    @RenderSection("Scripts", required: false)
                </div>
            </div>
        </div>
    </div>
<script>
   $(function(){
    var current = location.pathname;
    $('.dashboard-sidebar ul li a').each(function(){
        var $this = $(this);
        // if the current path is like this link, make it active
        if($this.attr('href').indexOf(current) !== -1){
            $this.addClass('active');
        }
    })
})
</script>
</body>
</html>
