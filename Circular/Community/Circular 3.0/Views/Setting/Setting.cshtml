@{
    var columnCount = 3;
     var columns = new List<List<string>>();
    @if(Model.lstCommunityCurrentPlan != null && Model.lstCommunityCurrentPlan.Count() > 0)
    {
        var featuresHtml = Model?.lstCommunityCurrentPlan?.FirstOrDefault()?.Features;
        // Use regular expressions to extract <li> elements
        var liRegex = new System.Text.RegularExpressions.Regex("<li>(.*?)</li>", System.Text.RegularExpressions.RegexOptions.Singleline);
        var liMatches = liRegex.Matches(featuresHtml);

       
        var itemsPerColumn = (int)Math.Ceiling(liMatches.Count / (double)columnCount);

        // Split the <li> elements into three lists
        
        for (int i = 0; i < columnCount; i++)
        {
            columns.Add(new List<string>());
        }

        for (int i = 0; i < liMatches.Count; i++)
        {
            columns[i % columnCount].Add(liMatches[i].Value);
        }
    }

}

